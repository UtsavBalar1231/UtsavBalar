{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro,.mdx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro,.mdx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,astro,md,mdx,json,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,astro,md,mdx,json,css}\"",
    "typecheck": "astro check && tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/mdx": "^4.3.5",
    "@astrojs/react": "^4.3.1",
    "@astrojs/sitemap": "^3.5.1",
    "@astrojs/tailwind": "^6.0.2",
    "@mdx-js/mdx": "^3.1.1",
    "astro": "^5.13.6",
    "astro-compress": "^2.3.8",
    "marked": "^16.2.1",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.3.1",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-tailwindcss": "^3.18.2",
    "postcss": "^8.5.6",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "tailwindcss": "3"
  }
}