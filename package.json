{
  "name": "website",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro,.mdx",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro,.mdx --fix",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,astro,md,mdx,json,css}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,astro,md,mdx,json,css}\"",
    "typecheck": "astro check && tsc --noEmit",
    "check": "npm run typecheck && npm run lint && npm run format:check"
  },
  "dependencies": {
    "@astrojs/check": "^0.9.5",
    "@astrojs/mdx": "^4.3.9",
    "@astrojs/sitemap": "^3.6.0",
    "@vercel/speed-insights": "^1.2.0",
    "astro": "^5.15.3",
    "astro-compress": "^2.3.8",
    "prettier": "^3.6.2",
    "prettier-plugin-astro": "^0.14.1",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@tailwindcss/vite": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-astro": "^1.4.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.16"
  }
}
